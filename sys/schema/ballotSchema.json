{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "location": {
      "type": "string"
    },
    "date": {
      "type": "string",
      "format": "date-time"
    },
    "name": {
      "type": "string"
    },
    "instructions": {
      "type": "string"
    },
    "sections": {
      "type": "array",
      "items": {
        "$ref": "ballotSectionSchema.json"
      }
    }
  },
  "required": [
    "location",
    "date",
    "name",
    "instructions",
    "sections"
  ]
}
